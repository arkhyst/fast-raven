{
    "name": "fast-raven/library",
    "description": "Lightweight, fast and minimalistic PHP framework for monolithic apps.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FastRaven\\": "src/"
        }
    },
    "require": {
        "php": "^8.5",
        "vlucas/phpdotenv": "^5.0"
    }, 
    "autoload-dev": {
        "psr-4": {
            "FastRaven\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml"
    }
}